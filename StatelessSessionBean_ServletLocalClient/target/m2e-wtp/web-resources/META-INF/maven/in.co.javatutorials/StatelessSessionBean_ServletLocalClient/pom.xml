<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>in.co.javatutorials</groupId>
	<artifactId>StatelessSessionBean_ServletLocalClient</artifactId>
	<packaging>war</packaging>
	<version>1.0</version>
	<name>StatelessSessionBean ServletLocalClient</name>
	<url>http://maven.apache.org</url>

	<build>
		<finalName>StatelessSessionBean_ServletLocalClient</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.apache.openejb</groupId>
			<artifactId>javaee-api</artifactId>
			<version>6.0-6</version>
		</dependency>

	</dependencies>

	

</project>
